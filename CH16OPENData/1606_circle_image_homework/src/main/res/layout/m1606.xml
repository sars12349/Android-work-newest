<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/darker_gray"
    android:padding="10dp"
    tools:context=".M1606">

    <tw.tcnr21.tcnrlibrary.CircleImgview
        android:id="@+id/circleImgview"
        android:layout_width="125dp"
        android:layout_height="129dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="48dp"
        android:layout_marginTop="74dp"
        app:srcCompat="@drawable/a1" />

    <tw.tcnr21.tcnrlibrary.CircleImgbutton
        android:id="@+id/circleImgbutton"
        android:layout_width="125dp"
        android:layout_height="140dp"
        android:layout_below="@+id/circleImgview"
        android:layout_marginStart="50dp"
        android:layout_marginTop="-128dp"
        android:layout_toEndOf="@+id/circleImgview"
        android:background="#00000000"
        app:srcCompat="@drawable/a2" />

    <EditText
        android:id="@+id/m1606_e001"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@+id/circleImgview"
        android:layout_marginTop="45dp"
        android:background="@android:drawable/editbox_background"
        android:hint="@string/m1606_e001"
        android:importantForAutofill="no"
        android:inputType="number"
        android:textAlignment="center"
        android:textSize="14sp"
        tools:targetApi="o" />

    <TextView
        android:id="@+id/m1606_ans"
        android:layout_width="match_parent"
        android:layout_height="63dp"
        android:layout_below="@+id/m1606_e001"
        android:layout_alignEnd="@+id/m1606_e001"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="1dp"
        android:background="@android:drawable/editbox_background"
        android:textAlignment="center" />

    <Button
        android:id="@+id/m1606_b001"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/m1606_ans"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:background="@android:drawable/btn_default_small"
        android:text="@string/m1606_b001"
        android:textStyle="bold" />

</RelativeLayout>